  "char_filter": {
    "street_number": {
      "pattern": "^(\\d+) .+",
      "type": "pattern_replace",
      "replacement": "$1"
    },
    "route": {
      "pattern": "(\\d+ +|^)([wsenWSEN] +|)(.+)",
      "type": "pattern_replace",
      "replacement": "$3"
    }
  },
  "analyzer": {
    "route_analyzer": {
      "filter": [
        "lowercase",
        "route_synonym"
      ],
      "char_filter": "route",
      "type": "custom",
      "tokenizer": "standard"
    },
    "street_number_analyzer": {
      "type": "custom",
      "char_filter": "street_number",
      "tokenizer": "keyword"
    },
    "uppercase_analyzer": {
      "filter": [
        "uppercase"
      ],
      "type": "custom",
      "tokenizer": "keyword"
    },
    "last_word_analyzer": {
      "filter": [
        "lowercase",
        "route_synonym"
      ],
      "char_filter": "route",
      "type": "custom",
      "tokenizer": "last_word_tokenizer"
    },
    "find_request_no_analyzer": {
      "type": "custom",
      "tokenizer": "find_request_no_tokenizer"
    }
  },
  "tokenizer": {
    "last_word_tokenizer": {
      "pattern": "\\s(\\w+)$",
      "type": "pattern",
      "group": "1"
    },
    "find_request_no_tokenizer": {
      "pattern": "\\|requestNo=([^\\|]+)\\|",
      "type": "pattern",
      "group": "1"
    }
  }
